{
  "name": "template",
  "version": "1.0.0",
  "description": "Base template for code camp",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "browserify -p tinyify src/index.js -o static/app.js",
    "develop": "concurrently \"watchify src/index.js -o static/app.js -v\" \"npm run serve\"",
    "serve": "node index.js",
    "start": "npm run build && npm run serve"
  },
  "author": "Will Wesley",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^7.2.2",
    "node-static": "^0.7.11",
    "stage-js": "^0.9.4",
    "tinyify": "^3.0.0",
    "watchify": "^4.0.0"
  }
}
